  "edition": "quran-uthmani-quran-academy",
  "surah": 37,
  "ayahs": {
    "75": "وَلَقَدۡ نَادَىٰنَا نُوحࣱ فَلَنِعۡمَ ٱلۡمُجِیبُونَ\n",
    "76": "وَنَجَّیۡنَـٰهُ وَأَهۡلَهُۥ مِنَ ٱلۡكَرۡبِ ٱلۡعَظِیمِ\n",
    "77": "وَجَعَلۡنَا ذُرِّیَّتَهُۥ هُمُ ٱلۡبَاقِینَ\n",
    "78": "وَتَرَكۡنَا عَلَیۡهِ فِی ٱلۡـَٔاخِرِینَ\n",
    "79": "سَلَـٰمٌ عَلَىٰ نُوحࣲ فِی ٱلۡعَـٰلَمِینَ\n",
    "80": "إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ\n",
    "81": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ\n",
    "82": "ثُمَّ أَغۡرَقۡنَا ٱلۡـَٔاخَرِینَ\n",
    "83": "۞ وَإِنَّ مِن شِیعَتِهِۦ لَإِبۡرَ ٰ⁠هِیمَ\n",
    "84": "إِذۡ جَاۤءَ رَبَّهُۥ بِقَلۡبࣲ سَلِیمٍ\n",
    "85": "إِذۡ قَالَ لِأَبِیهِ وَقَوۡمِهِۦ مَاذَا تَعۡبُدُونَ\n",
    "86": "أَىِٕفۡكًا ءَالِهَةࣰ دُونَ ٱللَّهِ تُرِیدُونَ\n",
    "87": "فَمَا ظَنُّكُم بِرَبِّ ٱلۡعَـٰلَمِینَ\n",
    "88": "فَنَظَرَ نَظۡرَةࣰ فِی ٱلنُّجُومِ\n",
    "89": "فَقَالَ إِنِّی سَقِیمࣱ\n",
    "90": "فَتَوَلَّوۡا۟ عَنۡهُ مُدۡبِرِینَ\n",
    "91": "فَرَاغَ إِلَىٰۤ ءَالِهَتِهِمۡ فَقَالَ أَلَا تَأۡكُلُونَ\n",
    "92": "مَا لَكُمۡ لَا تَنطِقُونَ\n",
    "93": "فَرَاغَ عَلَیۡهِمۡ ضَرۡبَۢا بِٱلۡیَمِینِ\n",
    "94": "فَأَقۡبَلُوۤا۟ إِلَیۡهِ یَزِفُّونَ\n",
    "95": "قَالَ أَتَعۡبُدُونَ مَا تَنۡحِتُونَ\n",
    "96": "وَٱللَّهُ خَلَقَكُمۡ وَمَا تَعۡمَلُونَ\n",
    "97": "قَالُوا۟ ٱبۡنُوا۟ لَهُۥ بُنۡیَـٰنࣰا فَأَلۡقُوهُ فِی ٱلۡجَحِیمِ\n",
    "98": "فَأَرَادُوا۟ بِهِۦ كَیۡدࣰا فَجَعَلۡنَـٰهُمُ ٱلۡأَسۡفَلِینَ\n",
    "99": "وَقَالَ إِنِّی ذَاهِبٌ إِلَىٰ رَبِّی سَیَهۡدِینِ\n",
    "100": "رَبِّ هَبۡ لِی مِنَ ٱلصَّـٰلِحِینَ\n",
    "101": "فَبَشَّرۡنَـٰهُ بِغُلَـٰمٍ حَلِیمࣲ\n",
    "102": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعۡیَ قَالَ یَـٰبُنَیَّ إِنِّیۤ أَرَىٰ فِی ٱلۡمَنَامِ أَنِّیۤ أَذۡبَحُكَ فَٱنظُرۡ مَاذَا تَرَىٰۚ قَالَ یَـٰۤأَبَتِ ٱفۡعَلۡ مَا تُؤۡمَرُۖ سَتَجِدُنِیۤ إِن شَاۤءَ ٱللَّهُ مِنَ ٱلصَّـٰبِرِینَ\n",
    "103": "فَلَمَّاۤ أَسۡلَمَا وَتَلَّهُۥ لِلۡجَبِینِ\n",
    "104": "وَنَـٰدَیۡنَـٰهُ أَن یَـٰۤإِبۡرَ ٰ⁠هِیمُ\n",
    "105": "قَدۡ صَدَّقۡتَ ٱلرُّءۡیَاۤۚ إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ\n",
    "106": "إِنَّ هَـٰذَا لَهُوَ ٱلۡبَلَـٰۤؤُا۟ ٱلۡمُبِینُ\n",
    "107": "وَفَدَیۡنَـٰهُ بِذِبۡحٍ عَظِیمࣲ\n",
    "108": "وَتَرَكۡنَا عَلَیۡهِ فِی ٱلۡـَٔاخِرِینَ\n",
    "109": "سَلَـٰمٌ عَلَىٰۤ إِبۡرَ ٰ⁠هِیمَ\n",
    "110": "كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ\n",
    "111": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ\n",
    "112": "وَبَشَّرۡنَـٰهُ بِإِسۡحَـٰقَ نَبِیࣰّا مِّنَ ٱلصَّـٰلِحِینَ\n",
    "113": "وَبَـٰرَكۡنَا عَلَیۡهِ وَعَلَىٰۤ إِسۡحَـٰقَۚ وَمِن ذُرِّیَّتِهِمَا مُحۡسِنࣱ وَظَالِمࣱ لِّنَفۡسِهِۦ مُبِینࣱ\n",
    "114": "وَلَقَدۡ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَـٰرُونَ\n",
    "115": "وَنَجَّیۡنَـٰهُمَا وَقَوۡمَهُمَا مِنَ ٱلۡكَرۡبِ ٱلۡعَظِیمِ\n",
    "116": "وَنَصَرۡنَـٰهُمۡ فَكَانُوا۟ هُمُ ٱلۡغَـٰلِبِینَ\n",
    "117": "وَءَاتَیۡنَـٰهُمَا ٱلۡكِتَـٰبَ ٱلۡمُسۡتَبِینَ\n",
    "118": "وَهَدَیۡنَـٰهُمَا ٱلصِّرَ ٰ⁠طَ ٱلۡمُسۡتَقِیمَ\n",
    "119": "وَتَرَكۡنَا عَلَیۡهِمَا فِی ٱلۡـَٔاخِرِینَ\n",
    "120": "سَلَـٰمٌ عَلَىٰ مُوسَىٰ وَهَـٰرُونَ\n",
    "121": "إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ\n",
    "122": "إِنَّهُمَا مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ\n",
    "123": "وَإِنَّ إِلۡیَاسَ لَمِنَ ٱلۡمُرۡسَلِینَ\n",
    "124": "إِذۡ قَالَ لِقَوۡمِهِۦۤ أَلَا تَتَّقُونَ\n",
    "125": "أَتَدۡعُونَ بَعۡلࣰا وَتَذَرُونَ أَحۡسَنَ ٱلۡخَـٰلِقِینَ\n",
    "126": "ٱللَّهَ رَبَّكُمۡ وَرَبَّ ءَابَاۤىِٕكُمُ ٱلۡأَوَّلِینَ\n",
    "127": "فَكَذَّبُوهُ فَإِنَّهُمۡ لَمُحۡضَرُونَ\n",
    "128": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ\n",
    "129": "وَتَرَكۡنَا عَلَیۡهِ فِی ٱلۡـَٔاخِرِینَ\n",
    "130": "سَلَـٰمٌ عَلَىٰۤ إِلۡ یَاسِینَ\n",
    "131": "إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ\n",
    "132": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ\n",
    "133": "وَإِنَّ لُوطࣰا لَّمِنَ ٱلۡمُرۡسَلِینَ\n",
    "134": "إِذۡ نَجَّیۡنَـٰهُ وَأَهۡلَهُۥۤ أَجۡمَعِینَ\n",
    "135": "إِلَّا عَجُوزࣰا فِی ٱلۡغَـٰبِرِینَ\n",
    "136": "ثُمَّ دَمَّرۡنَا ٱلۡـَٔاخَرِینَ\n",
    "137": "وَإِنَّكُمۡ لَتَمُرُّونَ عَلَیۡهِم مُّصۡبِحِینَ\n",
    "138": "وَبِٱلَّیۡلِۚ أَفَلَا تَعۡقِلُونَ\n",
    "139": "وَإِنَّ یُونُسَ لَمِنَ ٱلۡمُرۡسَلِینَ\n",
    "140": "إِذۡ أَبَقَ إِلَى ٱلۡفُلۡكِ ٱلۡمَشۡحُونِ\n",
    "141": "فَسَاهَمَ فَكَانَ مِنَ ٱلۡمُدۡحَضِینَ\n",
    "142": "فَٱلۡتَقَمَهُ ٱلۡحُوتُ وَهُوَ مُلِیمࣱ\n",
    "143": "فَلَوۡلَاۤ أَنَّهُۥ كَانَ مِنَ ٱلۡمُسَبِّحِینَ\n",
    "144": "لَلَبِثَ فِی بَطۡنِهِۦۤ إِلَىٰ یَوۡمِ یُبۡعَثُونَ\n",
    "145": "۞ فَنَبَذۡنَـٰهُ بِٱلۡعَرَاۤءِ وَهُوَ سَقِیمࣱ\n",
    "146": "وَأَنۢبَتۡنَا عَلَیۡهِ شَجَرَةࣰ مِّن یَقۡطِینࣲ\n",
    "147": "وَأَرۡسَلۡنَـٰهُ إِلَىٰ مِا۟ئَةِ أَلۡفٍ أَوۡ یَزِیدُونَ\n",
    "148": "فَـَٔامَنُوا۟ فَمَتَّعۡنَـٰهُمۡ إِلَىٰ حِینࣲ\n"
  }
}