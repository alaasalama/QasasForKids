{
  // Cloudflare Workers config to serve this static site with GA injection
  "name": "qasasforkids",
  "compatibility_date": "2025-09-04",
  "main": "worker.js",
  // Map your domain to this Worker (add both apex and www if used)
  "routes": [
    { "pattern": "qasasforkids.com/*", "zone_name": "qasasforkids.com" },
    { "pattern": "www.qasasforkids.com/*", "zone_name": "qasasforkids.com" }
  ],
  // Serve static assets from the repo root
  "assets": {
    "directory": "./"
  }
}
